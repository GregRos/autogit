{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Main (dist)",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "program": "${workspaceFolder}/publish/autogit.js",
      "cwd": "${workspaceFolder}/../__career",
      "args": ["watch"],
      "env": {
        "NODE_ENV": "development",
        "NODE_OPTIONS": "--enable-source-maps --stack-trace-limit=1000"
      }
    }
  ]
}
